{
  "name": "swaggular",
  "version": "0.2.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "swaggular": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexMA2/swaggular.git"
  },
  "bugs": {
    "url": "https://github.com/AlexMA2/swaggular/issues"
  },
  "homepage": "https://github.com/AlexMA2/swaggular#readme",
  "scripts": {
    "start": "node dist/index.js",
    "serve": "npm run build && node dist/index.js",
    "build": "tsc",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [
    "swagger",
    "openapi",
    "angular",
    "typescript",
    "code-generator"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "description": "A tool to generate Angular services and models from Swagger/OpenAPI specifications.",
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.1.0",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  },
  "dependencies": {
    "openapi-types": "^12.1.3",
    "yaml": "^2.8.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
